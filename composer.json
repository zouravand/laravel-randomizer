{
  "name": "tedon/laravel-randomizer",
  "description": "Generate random things but valid.",
  "license": "MIT",
  "authors": [
    {
      "name": "Pouya Zouravand",
      "email": "pouya.zuravand@gmail.com"
    }
  ],
  "type": "library",
  "keywords": [
    "Laravel",
    "Action",
    "LaravelAction",
    "Randomizer",
    "LaravelRandomizer",
    "Tedon"
  ],
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "orchestra/canvas": "^7.4",
    "orchestra/testbench": "^7.16"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tedon\\LaravelRandomizer\\Providers\\RandomizerServiceProvider"
      ],
      "aliases": {
        "Randomizer": "Tedon\\LaravelRandomizer\\Facades\\Menual"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Tedon\\LaravelRandomizer\\": "src"
    }
  },
  "autoload-dev": {
  },
  "minimum-stablity": "dev"
